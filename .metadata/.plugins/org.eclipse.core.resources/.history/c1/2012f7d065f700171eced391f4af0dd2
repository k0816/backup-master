	public String execute() {
		return SUCCESS;
	}

	partySearchList = partySearchDAO.getPartyInfo(party_info_transaction);

	Iterator<PartySearchDTO> iterator = partySearchList.iterator();

	if (!(iterator.hasNext())) {
		myPageList = null;

	String result = SUCCESS;
	return result;
}




